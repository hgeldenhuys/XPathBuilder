<?xml version="1.0" encoding="utf-8" ?>
<widget id="XPathBuilder.widget.XPathBuilder" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>XPathBuilder</name>
    <description>The description of this widget.</description>

    <icon>
        
    </icon>

    <properties>
        <property key="entityNameAttribute" type="attribute">
            <caption>Entity name</caption>
            <category>Data</category>
            <description>The name of the entity to browse from</description>
            <attributeTypes>
                <attributeType name="String"/>
                <attributeType name="Enum"/>
            </attributeTypes>
        </property>
        <property key="jsonAttribute" type="attribute">
            <caption>Json</caption>
            <category>Data</category>
            <description>Stores the JSON attribute data</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="xpathAttribute" type="attribute">
            <caption>Xpath</caption>
            <category>Data</category>
            <description>Stores the XPath attribute</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="notSymbol" type="translatableString" required="true">
            <caption>Not</caption>
            <category>Symbols</category>
            <description>Not operator symbol</description>
        </property>
        <property key="andSymbol" type="translatableString" required="true">
            <caption>and</caption>
            <category>Symbols</category>
            <description>And operator symbol</description>
        </property>
        <property key="orSymbol" type="translatableString" required="true">
            <caption>or</caption>
            <category>Symbols</category>
            <description>Or operator symbol</description>
        </property>
        <property key="andorSymbol" type="translatableString" required="true">
            <caption>andor</caption>
            <category>Symbols</category>
            <description>And/Or operator symbol</description>
        </property>
    </properties>
</widget>

